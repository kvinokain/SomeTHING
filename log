CREATE DATABASE loganalyzer;
CREATE USER 'loganalyzer'@'localhost' IDENTIFIED BY 'ваш_пароль';
GRANT ALL PRIVILEGES ON loganalyzer.* TO 'loganalyzer'@'localhost';
FLUSH PRIVILEGES;
EXIT;
wget http://download.adiscon.com/loganalyzer/loganalyzer-4.1.7.tar.gz
tar -xzf loganalyzer-4.1.7.tar.gz
sudo mv loganalyzer-4.1.7 /var/www/html/loganalyzer
sudo chown -R www-data:www-data /var/www/html/loganalyzer
